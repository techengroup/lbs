<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.2.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
	
	<reference id="meterService" interface="cn.techen.lbs.db.api.MeterService" />
	
	<reference id="mLbsService" interface="cn.techen.lbs.mm.api.MLbsService" />
	
	<reference id="mMeterService" interface="cn.techen.lbs.mm.api.MMeterService" />
	
	<reference id="mRegisterService" interface="cn.techen.lbs.mm.api.MRegisterService" />
	
	<reference id="mRelayService" interface="cn.techen.lbs.mm.api.MRelayService" />
	
	<reference id="mTaskService" interface="cn.techen.lbs.mm.api.MTaskService" />
	
	<reference id="protocolManagerService" interface="cn.techen.lbs.protocol.ProtocolManagerService" />

	<bean id="netContextBean" class="cn.techen.lbs.task.networking.common.NetContext">	
		<property name="meterService" ref="meterService"/>
		<property name="mLbsService" ref="mLbsService"/>	
		<property name="mMeterService" ref="mMeterService"/>
		<property name="mRegisterService" ref="mRegisterService"/>
		<property name="mRelayService" ref="mRelayService"/>
		<property name="mTaskService" ref="mTaskService"/>
		<property name="protocolManagerService" ref="protocolManagerService"/>
	</bean>
	
	<bean id="bootstrapBean" class="cn.techen.lbs.task.networking.Bootstrap" init-method="start">		
		<property name="context" ref="netContextBean"/>		
	</bean>
	
</blueprint>