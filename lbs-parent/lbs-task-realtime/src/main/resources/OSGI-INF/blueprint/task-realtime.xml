<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.2.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
	
	<reference id="mMeterService" interface="cn.techen.lbs.mm.api.MMeterService" />
	
	<reference id="mTaskService" interface="cn.techen.lbs.mm.api.MTaskService" />
	
	<reference id="protocolManagerService" interface="cn.techen.lbs.protocol.ProtocolManagerService" />

	<bean id="realTimeContextBean" class="cn.techen.lbs.task.realtime.common.RealTimeContext">
		<property name="mMeterService" ref="mMeterService"/>
		<property name="mByteService" ref="mTaskService"/>
		<property name="mTaskService" ref="mTaskService"/>
		<property name="mConfigService" ref="mTaskService"/>
		<property name="protocolManagerService" ref="protocolManagerService"/>
	</bean>
	
	<bean id="bootstrapBean" class="cn.techen.lbs.task.realtime.Bootstrap" init-method="start">		
		<property name="context" ref="realTimeContextBean"/>		
	</bean>
	
</blueprint>