<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.2.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

	<reference id="lbsService" interface="cn.techen.lbs.db.api.LbsService" />

	<reference id="meterService" interface="cn.techen.lbs.db.api.MeterService" />
	
	<reference id="fnService" interface="cn.techen.lbs.db.api.FnService" />
	
	<reference id="reportService" interface="cn.techen.lbs.db.api.ReportService" />
	
	<reference id="mBaseService" interface="cn.techen.lbs.mm.api.MBaseService" />
	
	<reference id="mMeterService" interface="cn.techen.lbs.mm.api.MMeterService" />
	
	<reference id="mRegisterService" interface="cn.techen.lbs.mm.api.MRegisterService" />
	
	<reference id="mRelayService" interface="cn.techen.lbs.mm.api.MRelayService" />
	
	<reference id="mReportService" interface="cn.techen.lbs.mm.api.MReportService" />
	
	<bean id="mysql2RedisBean" class="cn.techen.lbs.data.manager.Mysql2Redis">
		<property name="lbsService" ref="lbsService"/>
		<property name="meterService" ref="meterService"/>
		<property name="fnService" ref="fnService"/>
		<property name="reportService" ref="reportService"/>
		<property name="mBaseService" ref="mBaseService"/>
		<property name="mMeterService" ref="mMeterService"/>
		<property name="mRegisterService" ref="mRegisterService"/>
		<property name="mRelayService" ref="mRelayService"/>
		<property name="mReportService" ref="mReportService"/>
	</bean>	
	
	<bean id="gis2NetBean" class="cn.techen.lbs.data.manager.GIS2Net">
		<property name="meterService" ref="meterService"/>
	</bean>	
	
	<bean id="bootStrapBean" class="cn.techen.lbs.data.Bootstrap" init-method="start">
		<property name="mysql2Redis" ref="mysql2RedisBean"/>
		<property name="gis2Net" ref="gis2NetBean"/>
	</bean>
	
</blueprint>