<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.2.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

	<reference id="storeService" interface="cn.techen.lbs.db.api.StoreService" />
	
	<reference id="mTableService" interface="cn.techen.lbs.mm.api.MTableService" />
	
	<bean id="redis2MysqlBean" class="cn.techen.lbs.store.Redis2Mysql">
		<property name="storeService" ref="storeService"/>
		<property name="mTableService" ref="mTableService"/>
	</bean>	
	
	<bean id="entranceBean" class="cn.techen.lbs.data.Entrance" init-method="start">
		<property name="redis2Mysql" ref="redis2MysqlBean"/>
	</bean>
	
</blueprint>